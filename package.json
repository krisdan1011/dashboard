{
  "name": "dashboard",
  "version": "0.1.0",
  "description": "bespoken dashboard",
  "repository": "https://github.com/bespoken/bespoken-dashboard",
  "main": "./dist/bundle.js",
  "scripts": {
    "postinstall": "typings install",
    "clean": "node scripts/clean.js",
    "lint": "tslint src/**/*.{ts,tsx}",
    "pretest": "tsc && npm run lint",
    "test": "mocha src/**/*.test.js",
    "precoverage": "npm run pretest",
    "coverage": "istanbul cover --include-all-sources --root src/ -x \"**/*.test.js\" _mocha src/**/*.test.js -- -R spec && remap-istanbul -i ./coverage/coverage.json -o ./coverage/ts-report -t html && ./node_modules/.bin/remap-istanbul -i coverage/coverage.json -t lcovonly > coverage/lcov-ts.info",
    "coveralls": "npm run coverage && ./node_modules/coveralls/bin/coveralls.js < coverage/lcov-ts.info",
    "build": "webpack",
    "start": "node scripts/server.js"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "autoprefixer-core": "^6.0.1",
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "css-loader": "^0.25.0",
    "del": "^2.2.2",
    "enzyme": "^2.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "postcss-loader": "^0.13.0",
    "react-addons-test-utils": "^15.3.2",
    "remap-istanbul": "^0.6.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "typings": "^1.3.2",
    "typings-for-css-modules-loader": "^0.2.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-persist": "^4.0.0-alpha6",
    "redux-thunk": "^2.1.0"
  }
}
