{
  "name": "bespoken-dashboard",
  "version": "0.5.0",
  "description": "The dashboard for https://bespoken.tools",
  "repository": "https://github.com/bespoken/dashboard",
  "main": "./dist/scripts/dashboard.js",
  "scripts": {
    "postinstall": "typings install",
    "clean": "node scripts/clean.js",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "pretest": "tsc && npm run lint",
    "test": "mocha 'src/**/*.test.js'",
    "precoverage": "npm run pretest",
    "coverage": "istanbul cover --include-all-sources --root src/ -x \"**/*test.js\" _mocha \"src/**/*.test.js\" -- -R spec && remap-istanbul -i ./coverage/coverage.json -o ./coverage/ts-report -t html && ./node_modules/.bin/remap-istanbul -i coverage/coverage.json -t lcovonly > coverage/lcov-ts.info",
    "coveralls": "npm run coverage && ./node_modules/coveralls/bin/coveralls.js < coverage/lcov-ts.info",
    "start": "node scripts/server.js",
    "preversion": "webpack && git add . && git commit -m 'dist-builds'",
    "postversion": "git push && git push --tags",
    "build": "webpack"
  },
  "contributors": [
    {
      "name": "Michael Myers",
      "email": "michael@bespoken.tools"
    },
    {
      "name": "Chris Dietz"
    },
    {
      "name": "Umut Genlik"
    },
    {
      "name": "Ling Hung"
    }
  ],
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "browser-sync": "^2.17.5",
    "bs-fullscreen-message": "^1.1.0",
    "chai": "^3.5.0",
    "connect-history-api-fallback": "^1.3.0",
    "coveralls": "^2.11.14",
    "css-loader": "^0.25.0",
    "del": "^2.2.2",
    "enzyme": "^2.5.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^5.5.0",
    "firebase": "^3.5.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "postcss-loader": "^0.13.0",
    "react-addons-test-utils": "^15.3.2",
    "redux-mock-store": "^1.2.1",
    "remap-istanbul": "^0.7.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.1.5",
    "strip-ansi": "^3.0.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.9.5",
    "tslint": "^3.15.1",
    "tslint-react": "^1.1.0",
    "typescript": "^2.0.6",
    "typings": "^1.3.2",
    "typings-for-css-modules-loader": "^1.0.0",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "es6-promise": "^4.0.5",
    "history": "^2.1.2",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.15.2",
    "object-assign": "^4.1.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-ga": "^2.1.2",
    "react-json-tree": "^0.10.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-persist": "^4.0.0-alpha6",
    "redux-thunk": "^2.1.0",
    "uuid": "^2.0.3"
  }
}
