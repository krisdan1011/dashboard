{
  "name": "dashboard",
  "version": "0.0.1",
  "description": "bespoken dashboard",
  "repository": "https://github.com/bespoken/bespoken-dashboard",
  "main": "./dist/bundle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "node scripts/clean.js",
    "postinstall": "typings install",
    "lint": "tslint src/**/*.ts",
    "pretest": "tsc && npm run lint",
    "test": "mocha src/**/*.test.js",
    "precoverage": "npm run pretest",
    "coverage": "istanbul cover --include-all-sources --root src/ -x \"**/*.test.js\" _mocha src/**/*.test.js -- -R spec && remap-istanbul -i ./coverage/coverage.json -o ./coverage/ts-report -t html && ./node_modules/.bin/remap-istanbul -i coverage/coverage.json -t lcovonly > coverage/lcov-ts.info",
    "coveralls": "npm run coverage && coveralls < coverage/lcov-ts.info",
    "build": "gulp",
    "demo": "webpack-dev-server --hot --inline --hot --colors --progress --port 8723 --config webpack.config.js --content-base app/"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "apache-server-configs": "^2.7.1",
    "babel-core": "^6.0.15",
    "babel-preset-es2015": "^6.0.15",
    "browser-sync": "^2.9.0",
    "browserify": "^13.1.0",
    "bs-fullscreen-message": "^1.0.0",
    "chai": "^3.5.0",
    "compression": "1.6.1",
    "connect-history-api-fallback": "^1.3.0",
    "del": "^2.2.2",
    "eslint-config-google": "^0.3.0",
    "glob": "^7.1.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.0.0",
    "gulp-cache": "0.2.2",
    "gulp-concat": "^2.5.2",
    "gulp-cssnano": "^2.0.0",
    "gulp-eslint": "^1.0.0",
    "gulp-htmlmin": "^1.3.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.0.0",
    "gulp-load-plugins": "^1.0.0",
    "gulp-mocha": "^3.0.1",
    "gulp-newer": "^1.0.0",
    "gulp-sass": "^2.0.0",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-tslint": "^6.1.1",
    "gulp-typescript": "^3.0.1",
    "gulp-uglify": "^1.0.1",
    "gulp-useref": "^3.0.0",
    "istanbul": "^0.4.5",
    "karma": "^1.3.0",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "psi": "^2.0.2",
    "react-addons-test-utils": "^15.3.2",
    "react-lite": "0.15.3",
    "run-sequence": "^1.0.1",
    "source-map-loader": "^0.1.5",
    "strip-ansi": "^3.0.0",
    "sw-precache": "^3.2.0",
    "sw-toolbox": "^3.0.1",
    "ts-loader": "^0.8.2",
    "tsify": "^1.0.7",
    "tslint": "^3.15.1",
    "tslint-react": "^1.0.0",
    "typescript": "^2.0.3",
    "typings": "^1.3.2",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.2.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
