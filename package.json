{
  "name": "dashboard",
  "version": "0.0.1",
  "description": "bespoken dashboard",
  "repository": "https://github.com/bespoken/bespoken-dashboard",
  "main": "./dist/bundle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "postinstall": "typings install",
    "clean": "node scripts/clean.js",
    "lint": "tslint src/**/*.ts",
    "pretest": "tsc && npm run lint",
    "test": "mocha src/**/*.test.js",
    "precoverage": "npm run pretest",
    "coverage": "istanbul cover --include-all-sources --root src/ -x \"**/*.test.js\" _mocha src/**/*.test.js -- -R spec && remap-istanbul -i ./coverage/coverage.json -o ./coverage/ts-report -t html && ./node_modules/.bin/remap-istanbul -i coverage/coverage.json -t lcovonly > coverage/lcov-ts.info",
    "coveralls": "npm run coverage && coveralls < coverage/lcov-ts.info",
    "build": "webpack",
    "server": "node scripts/server.js"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "apache-server-configs": "^2.7.1",
    "babel-core": "^6.0.15",
    "babel-preset-es2015": "^6.0.15",
    "browser-sync": "^2.9.0",
    "browserify": "^13.1.0",
    "bs-fullscreen-message": "^1.0.0",
    "chai": "^3.5.0",
    "compression": "1.6.1",
    "connect-history-api-fallback": "^1.3.0",
    "del": "^2.2.2",
    "eslint-config-google": "^0.3.0",
    "istanbul": "^0.4.5",
    "karma": "^1.3.0",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "psi": "^2.0.2",
    "react-addons-test-utils": "^15.3.2",
    "react-lite": "0.15.3",
    "remap-istanbul": "^0.6.4",
    "run-sequence": "^1.0.1",
    "source-map-loader": "^0.1.5",
    "strip-ansi": "^3.0.0",
    "sw-precache": "^3.2.0",
    "sw-toolbox": "^3.0.1",
    "ts-loader": "^0.8.2",
    "tsify": "^1.0.7",
    "tslint": "^3.15.1",
    "tslint-react": "^1.0.0",
    "typescript": "^2.0.3",
    "typings": "^1.3.2",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "material-design-lite": "^1.2.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
}
