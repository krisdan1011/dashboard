{
  "name": "bespoken-dashboard",
  "version": "0.2.0",
  "description": "The dashboard for https://bespoken.tools",
  "repository": "https://github.com/bespoken/dashboard",
  "main": "./dist/scripts/dashboard.js",
  "scripts": {
    "postinstall": "typings install",
    "clean": "node scripts/clean.js",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "pretest": "tsc && npm run lint",
    "test": "mocha 'src/**/*.test.js'",
    "precoverage": "npm run pretest",
    "coverage": "istanbul cover --include-all-sources --root src/ -x \"**/*.test.js\" _mocha \"src/**/*.test.js\" -- -R spec && remap-istanbul -i ./coverage/coverage.json -o ./coverage/ts-report -t html && ./node_modules/.bin/remap-istanbul -i coverage/coverage.json -t lcovonly > coverage/lcov-ts.info",
    "coveralls": "npm run coverage && ./node_modules/coveralls/bin/coveralls.js < coverage/lcov-ts.info",
    "build": "webpack && git add . && git commit -m dist",
    "start": "node scripts/server.js",
    "preversion": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "autoprefixer-core": "^6.0.1",
    "browser-sync": "^2.17.3",
    "bs-fullscreen-message": "^1.1.0",
    "chai": "^3.5.0",
    "connect-history-api-fallback": "^1.3.0",
    "coveralls": "^2.11.14",
    "css-loader": "^0.25.0",
    "del": "^2.2.2",
    "enzyme": "^2.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^5.5.0",
    "firebase": "^3.4.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "postcss-loader": "^0.13.0",
    "react-addons-test-utils": "^15.3.2",
    "redux-mock-store": "^1.2.1",
    "remap-istanbul": "^0.6.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.1.5",
    "strip-ansi": "^3.0.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "tslint-react": "^1.0.0",
    "typescript": "^2.0.3",
    "typings": "^1.3.2",
    "typings-for-css-modules-loader": "^0.2.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "es6-promise": "^4.0.5",
    "history": "^2.1.2",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "object-assign": "^4.1.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-json-tree": "^0.10.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-persist": "^4.0.0-alpha6",
    "redux-thunk": "^2.1.0"
  }
}
